@{
    ViewData["Title"] = "Home Page";
}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item active">
            <img src="https://cdn.vox-cdn.com/thumbor/HnaKZcEUqFLydfXqzK5mrR8yYJk=/0x0:2000x1000/fit-in/1200x600/cdn.vox-cdn.com/uploads/chorus_asset/file/7104451/eatersea0916_pike_place_market_shutterstock_mcarter.0.jpg" alt="1">
        </div>

        <div class="item">
            <img src="http://clubedaquimica.com/wp-content/uploads/2019/05/frutas.jpg" alt="2">
        </div>

        <div class="item">
            <img src="http://www.supermercadista.com.br/wp-content/uploads/2016/10/condominio.jpg" alt="2">
        </div>
        @*<div class="item">
            <img src="~/images/banner1.svg" alt="New York">
        </div>*@
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Anterior</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Próximo</span>
    </a>
</div>

